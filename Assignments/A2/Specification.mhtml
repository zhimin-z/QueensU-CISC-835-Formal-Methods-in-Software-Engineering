   content: <span class=3D"pl-k">lone</span> Val                          <=
span class=3D"pl-c">// a location contains at most one value</span>
}
<span class=3D"pl-k">sig</span> SLoc <span class=3D"pl-k">extends</span> Lo=
c {}                      =20
<span class=3D"pl-k">sig</span> HLoc <span class=3D"pl-k">extends</span> Lo=
c {}                      =20

<span class=3D"pl-k">sig</span> Var {
   sloc: <span class=3D"pl-k">one</span> SLoc                             <=
span class=3D"pl-c">// a variable has exactly one stack location</span>
}

<span class=3D"pl-k">abstract</span> <span class=3D"pl-k">sig</span> Val {}=
